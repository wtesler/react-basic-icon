{
  "name": "react-basic-icon",
  "version": "1.1.2",
  "description": "An image element that is easier to work with than a regular <img/> element",
  "scripts": {
    "build": "npm run clean && babel src/lib --out-dir dist --copy-files",
    "clean": "rimraf dist"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^18"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "rimraf": "3.0.2"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": []
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "keywords": [
    "react",
    "img",
    "icon"
  ],
  "author": "Will Tesler",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wtesler/react-basic-icon.git"
  },
  "homepage": "https://github.com/wtesler/react-basic-icon"
}
